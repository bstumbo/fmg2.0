{#
/**
 * @file bar.html.twig
 * Default theme implementation to present Bar data.
 *
 * This template is used when viewing Bar pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_bar()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('bar') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
